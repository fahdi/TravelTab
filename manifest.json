{
  "manifest_version": 3,
  "name": "TravelTab",
  "version": "1.0",
  "description": "Displays a new travel photo each time you open a new tab.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "",
    "default_icon": "icons/icon48.png"
  }
}